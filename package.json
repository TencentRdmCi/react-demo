{
  "name": "newci_react",
  "version": "0.1.0",
  "description": "The newci's react version",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js",
    "lint:tests": "eslint ./tests/**/*.spec.js",
    "compile": "webpack",
    "start": "npm run dev",
    "dev": "webpack-dev-server --hot --inline --progress --colors --quiet false --noInfo false --port 3000",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run lint && npm run test && npm run compile"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": "^2.0.0",
    "bootstrap": "^3.0.0",
    "history": "^1.9.0",
    "humps": "^0.6.0",
    "isomorphic-fetch": "^2.1.1",
    "material-ui": "^0.13.3",
    "normalizr": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.2",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "lodash": "^3.10.1",
    "chalk": "^1.1.1",
    "dotenv": "^1.2.0",
    "yargs": "^3.18.0",
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-eslint": "^4.1.1",
